{
  "name": "test-pkg",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rm -rf dist",
    "prepack": "tsup"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./schemas": {
      "import": "./dist/schemas.js",
      "require": "./dist/schemas.cjs"
    }
  },
  "tsup": {
    "entry": {
      "index": "./src/index.ts",
      "schemas": "./src/schemas/index.ts"
    },
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true
  },
  "devDependencies": {
    "tsmpkg": "workspace:^",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "private": true,
  "volta": {
    "extends": "../../package.json"
  }
}
