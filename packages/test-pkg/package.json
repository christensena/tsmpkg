{
  "name": "test-pkg",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./schemas": {
      "import": "./dist/schemas.js",
      "require": "./dist/schemas.cjs"
    }
  },
  "tsup": {
    "entry": {
      "index": "./src/index.ts",
      "schemas": "./src/schemas/index.ts"
    },
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true
  },
  "devDependencies": {
    "tsmpkg": "^0.0.5",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "private": true,
  "volta": {
    "extends": "../../package.json"
  }
}
